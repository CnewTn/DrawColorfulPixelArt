       }else if(matrix[j][i] == 7){
         fill(0,0,255);
       }else if(matrix[j][i] == 8){
         fill(5,100,30);
       }
       
      // Do not change any lines of code found below this comment
      rect(i*pixelSize, j*pixelSize, pixelSize, pixelSize); 
   }
  } 
