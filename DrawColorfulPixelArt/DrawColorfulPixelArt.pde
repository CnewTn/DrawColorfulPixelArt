         fill(255, 255, 255);
       }else if (matrix[j][i] == 1){
         fill(0, 0, 0); 
       }else if (matrix[j][i] == 2){
         fill(128, 128, 128);
       }else if(matrix[j][i] == 3){
         fill(255,255,0);
       }else if(matrix[j][i] == 4){
         fill(0,255,255);
